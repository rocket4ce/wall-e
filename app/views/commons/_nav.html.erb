<ul>
  <% if user_signed_in? %>
    <li>
      <%= current_user.email %>
    </li>
    <li>
      <%= link_to('Logout', destroy_user_session_path, method: :delete) %>
    </li>
    <% else %>
    <li>
      <%= link_to "Sign in", new_user_session_path %><br />
    </li>

    <li>
      <%= link_to "Sign up", new_user_registration_path %><br />
    </li>
  <% end %>
</ul>
